@article{archer1995,
  abstract = {OBJECTIVE: The pharmacokinetics of a 100 mg vaginal progesterone suppository was evaluated on days 1 and 7 and a 200 mg suppository on day 14. All the volunteers were given oral 17$\beta$-estradiol during the study. STUDY DESIGN: Ten postmenopausal women volunteered for this study. Progesterone was given as a vaginal suppository. Peripheral venous samples were obtained at appropriate intervals and analyzed for 17$\beta$-estradiol and progesterone levels. Area under the curve for progesterone was assessed by the trapezoidal method. Statistical analysis was performed by a one-way analysis of variance. RESULTS: Serum 17$\beta$-estradiol leels ranged from 22 to 182 pg/ml. Maximal serum progesterone levels ranged from 5.7 to 20.9 ng/ml, with the mean maximal levels 13.97, 16.09, and 12.68 ng/ml (not significantly different) and a mean area under the curve of 168.13, 207.64 and 227.71 ng/ml per hour on days 1, 7, and 14 (not statistically different). CONCLUSIONS: These data indicate that vaginal absorption of progesterone is efficient. The lack of difference in the area under the curve for both doses suggests that the vaginal mucosa or the total surface area of the vagina may limit the absorption of progesterone from the vagina.},
  author   = {Archer, David F. and Fahy, George E. and Viniegra-Sibal, Amabel and Anderson, Freedolph D. and Snipes, Wallace and Foldesy, Robin G.},
  doi      = {10.1016/0002-9378(95)90268-6},
  file     = {:home/dewittp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Archer et al. - 1995 - Initial and steady-state pharmacokinetics of a vaginally administered formulation of progesterone.pdf:pdf},
  issn     = {00029378},
  journal  = {American Journal of Obstetrics and Gynecology},
  keywords = {Progesterone,pharmacokinetics,vaginal},
  month    = aug,
  number   = {2},
  pages    = {471--478},
  title    = {{Initial and steady-state pharmacokinetics of a vaginally administered formulation of progesterone}},
  url      = {http://www.sciencedirect.com/science/article/pii/0002937895902686},
  volume   = {173},
  year     = {1995}
}

